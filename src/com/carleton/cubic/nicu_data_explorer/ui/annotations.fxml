<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="431.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121">
    <TitledPane animated="false" text="Annotation Log">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="536.0" prefWidth="598.0">
             <TableView fx:id="annotationTable" editable="true" prefHeight="350.0" prefWidth="600.0" tableMenuButtonVisible="true">
                 <columns>
                     <TableColumn fx:id="category" prefWidth="110.0" text="Category" />
                     <TableColumn fx:id="name" prefWidth="110.0" text="Name" />
                     <TableColumn fx:id="displayStartTime" prefWidth="60.0" text="Start" />
                     <TableColumn fx:id="displayEndTime" prefWidth="60.0" text="Stop" />
                 </columns>
             </TableView>
        </AnchorPane>
    </TitledPane>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
          <Button fx:id="addAnnotationButton" mnemonicParsing="false" text="Add Annotation">
            <HBox.margin>
                <Insets left="20.0"/>
            </HBox.margin></Button>
          <Button fx:id="editAnnotationButton" layoutX="30.0" layoutY="10.0" mnemonicParsing="false"
                  text="Edit Annotation">
            <HBox.margin>
               <Insets left="20.0" />
            </HBox.margin></Button>
          <Button fx:id="deleteAnnotationButton" mnemonicParsing="false" text="Delete Annotation">
            <HBox.margin>
               <Insets left="20.0" />
            </HBox.margin></Button>
      </children>
       <VBox.margin>
           <Insets bottom="3.0"/>
       </VBox.margin>
   </HBox>
    <HBox prefHeight="100.0" prefWidth="200.0">
        <children>
            <Button fx:id="playAllButton" mnemonicParsing="false" text="Play All">
                <HBox.margin>
                    <Insets left="40.0"/>
                </HBox.margin>
            </Button>
            <Button fx:id="saveUpdatesButton" mnemonicParsing="false" text="Save Updates">
            <HBox.margin>
               <Insets left="20.0" />
            </HBox.margin></Button>
            <Button fx:id="saveSessionButton" mnemonicParsing="false" text="Save File">
            <HBox.margin>
               <Insets left="20.0" />
            </HBox.margin>
            </Button>
      </children>
   </HBox>
</VBox>
